{
  "name": "@garyleefight/blowfish-astro-theme",
  "version": "0.1.2",
  "description": "Blowfish-inspired Astro theme package with compatibility-focused component APIs.",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./styles.css": "./src/styles/theme.css",
    "./components/*": "./src/components/*",
    "./layouts/*": "./src/layouts/*",
    "./sync-assets": "./scripts/sync-public-assets.mjs"
  },
  "files": [
    "src",
    "public",
    "scripts",
    "tailwind.config.js"
  ],
  "bin": {
    "blowfish-astro-sync": "./scripts/sync-public-assets.mjs"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "check": "astro check"
  },
  "peerDependencies": {
    "astro": ">=5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "astro",
    "theme",
    "blowfish",
    "static-site"
  ],
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-stringify": "^10.0.1",
    "rehype-slug": "^6.0.0",
    "remark-code-title": "^0.2.5",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "unified": "^11.0.5"
  }
}
